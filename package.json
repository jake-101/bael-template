    "node-sass": "^4.9.2",
    "nuxt": "^2.0.0",
    "vue-lazyload": "^1.3.0",
    "vue-moment": "^4.0.0",
    "vue-smooth-reflow": "^0.1.12"
  },
  "devDependencies": {
    "@sendgrid/client": "^6.3.0",
    "string.prototype.includes": "^2.0.0",
    "sass-loader": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.0",
    "prettier": "^2.1.1"
